{
  "name": "zireael",
  "version": "0.7.0",
  "description": "Lightweight automation tool",
  "license": "MIT",
  "repository": "resure/zireael",
  "author": {
    "name": "Gadzhi Gadzhiev",
    "email": "inbox@resure.net",
    "url": "zireael.org"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && ava",
    "start": "node app.js",
    "setup": "rm -rf node_modules && npm install && npm run make",
    "make": "./node_modules/.bin/enb make -n",
    "watch": "DEBUG=server nodemon index.js"
  },
  "xo": {
    "ignores": [
      "libs/**",
      "bundles/**"
    ]
  },
  "files": [
    "app",
    "bundles",
    "config",
    "public",
    "app.js",
    "index.js"
  ],
  "keywords": [
    "automation",
    "user-scripts"
  ],
  "dependencies": {
    "body-parser": "1.x.x",
    "cookie-parser": "1.x.x",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-handlebars": "^3.0.0",
    "morgan": "1.x.x",
    "nodemon": "^1.9.1",
    "serve-favicon": "~2.3.0",
    "ym": "^0.1.2"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "xo": "^0.12.1",
    "bower": "^1.5.3",
    "enb": "^1.1.2",
    "enb-bem-techs": "^2.2.1",
    "enb-borschik": "^2.3.0",
    "borschik-tech-cleancss": "^2.1.0",
    "enb-js": "^1.1.0",
    "enb-modules": "^0.2.0",
    "enb-stylus": "^2.0.0",
    "enb-bh": "^1.1.0",
    "bem-core": "bem/bem-core#v2.9.0",
    "bem-components": "bem/bem-components#v2.4.0"
  }
}
